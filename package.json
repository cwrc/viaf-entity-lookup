{
  "name": "viaf-entity-lookup",
  "version": "0.0.0-development",
  "description": "Find entities (people, places, organizations) in VIAF.",
  "main": "src/index.js",
  "scripts": {
    "test": "",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "precommit": "npm run test:single && npm run check-coverage",
    "cm": "git-cz",
    "test:single": "nyc tape test/index.js",
    "check-coverage": "nyc check-coverage --branches 100 --functions 100 --lines 100",
    "publish-coverage": "codecov --token=bfcccbea-ae1e-4b76-bfa1-83be70525b9a",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov"


  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cwrc/viaf-entity-lookup.git"
  },
  "keywords": [
    "viaf",
    "entity",
    "linked-data",
    "sparql"
  ],
  "author": "James Chartrand",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/cwrc/viaf-entity-lookup/issues"
  },
  "homepage": "https://github.com/cwrc/viaf-entity-lookup#readme",
  "devDependencies": {
    "codecov.io": "^0.1.6",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "faucet": "0.0.1",
    "husky": "^0.14.3",
    "nock": "^9.0.14",
    "nyc": "^11.1.0",
    "path": "^0.12.7",
    "semantic-release": "^6.3.6",
    "tape": "^4.8.0",
    "tape-nock": "^1.6.0",
    "watch": "^1.0.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "viaf-wrapper": "^0.1.4"
  }
}
